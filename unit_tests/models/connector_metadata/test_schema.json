{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["dockerRepository", "dockerImageTag"],
      "properties": {
        "dockerRepository": {
          "type": "string"
        },
        "dockerImageTag": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
